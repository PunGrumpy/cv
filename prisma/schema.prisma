generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Resume {
  id                 Int    @id @default(autoincrement())
  name               String
  initials           String
  location           String
  locationUrl        String
  about              String
  summary            String
  avatarUrl          String
  personalWebsiteUrl String
  contact            Json
  education          Json
  work               Json
  skills             Json
  projects           Json
}
